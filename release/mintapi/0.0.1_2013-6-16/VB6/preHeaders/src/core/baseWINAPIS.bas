#If Not REMOVE_API_SECTION_ADITIONALS Then
    Public Type API_FILE
        HANDLE As Long
        Path As String * 255
    End Type
    Public Function API_ReadINIFile(Path As String, Section As String, Value As Variant, Default As Variant, Optional IsColor As Boolean = False, Optional ConfigurationMusic As Boolean = False) As Variant
        Dim ColorArr As Variant
        Dim str As String
        
        If ConfigurationMusic = True Then
            str = String(255, Chr(0))
            str = Left(str, API_GetPrivateProfileString(Section, ByVal Value, 0, str, Len(str), Path))
            If str = 0 Then
                API_ReadINIFile = Trim(Default)
            Else
                API_ReadINIFile = Trim(str)
            End If
            Exit Function
        End If
        
        If IsColor = True Then
            str = String(255, Chr(0))
            str = Left(str, API_GetPrivateProfileString(Section, ByVal Value, 0, str, Len(str), Path))
            
            If str = 0 Then
                API_ReadINIFile = Default
            Else
                ColorArr = Split(str, ",")
                If UBound(ColorArr) <> 2 Then
                    API_ReadINIFile = Default
                Else
                    API_ReadINIFile = RGB(ColorArr(0), ColorArr(1), ColorArr(2))
                End If
            End If
        Else
            str = String(255, Chr(0))
            str = Left(str, API_GetPrivateProfileString(Section, ByVal Value, 0, str, Len(str), Path))
            If str = 0 Then
                API_ReadINIFile = Trim(Default)
            Else
                API_ReadINIFile = Trim(str)
            End If
        End If
    End Function
    Public Function API_WriteINIFile(Path As String, Section As String, KeyName As String, KeyValue As String, Optional IsColor As Boolean = False, Optional ConfigurationMusic As Boolean = False) As Boolean
        Dim Ret As Long
        Ret = API_WritePrivateProfileString(Section, KeyName, KeyValue, Path)
        If Ret = 0 Then
            API_WriteINIFile = True
        Else
            API_WriteINIFile = False
        End If
    End Function
    Public Function API_InitializeCommonControls() As Boolean
       Dim iccex As API_tagInitCommonControlsEx
       ' Ensure CC available:
       With iccex
           .lngICC = API_ICC_USEREX_CLASSES
           .lngSize = LenB(iccex)
       End With
       Call API_InitCommonControlsEx(iccex)
       API_InitializeCommonControls = (err.Number = 0)
    End Function
    Public Function EnableVisualStyles(): EnableVisualStyles = API_InitializeCommonControls: Exit Function
    Public Function API_SearchFile(ByVal strFilename As String, ByVal strRootPath As String) As String
        Dim strBuffer As String
        Dim lngResult As Long
        Const MAX_PATH = 260
        API_SearchFile = ""
        strBuffer = String$(MAX_PATH, 0)
        lngResult = API_SearchTreeForFile(strRootPath, strFilename, strBuffer)
        If lngResult <> 0 Then
            If InStr(strBuffer, vbNullChar) > 0 Then
                API_SearchFile = Left$(strBuffer, InStr(strBuffer, vbNullChar) - 1)
            End If
        End If
    End Function
    Public Sub API_cleanLastError(): Call API_SetLastError(API_SUCCESSFULL): End Sub
    Public Sub API_throwLastError()
        Dim lError As Long
        lError = API_GetLastError
        If lError <> API_SUCCESSFULL Then
            Call err.Raise(lError)
        End If
    End Sub
